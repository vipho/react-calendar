{
  "presets": [
    "@babel/preset-env"
  ],
  "plugins": [
    "babel-plugin-transform-react-pug",
    "@babel/plugin-proposal-object-rest-spread",
    ["@babel/plugin-proposal-decorators", { "legacy": true}],
    ["@babel/plugin-proposal-class-properties", { "loose": true}],
    ["@babel/plugin-transform-react-jsx"]
  ]
}
